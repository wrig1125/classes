> # Calculate effect size (Cohen's d)
> mean_diff <- mean(vegetarians$shannon) - mean(omnivores$shannon)
> pooled_sd <- sqrt(((length(vegetarians$shannon) - 1) * var(vegetarians$shannon) +
+                        (length(omnivores$shannon) - 1) * var(omnivores$shannon)) /
+                       (length(vegetarians$shannon) + length(omnivores$shannon) - 2))
> effect_size <- mean_diff / pooled_sd
